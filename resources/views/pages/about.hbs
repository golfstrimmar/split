<html lang="en">
<head>
  {{> ../common/head }}
</head>
<body class="about">
{{> ../common/header-white}}
<section class="we-do__hero">
  <div class="container">
    <div class="row no-gutters align-items-md-center ">
      <div class=" we-do__hero__title ">
        <h3>
            Meet our glorious team, who 
<span id="typed" data-typed-text="make impossible, make impossible, make awesome, make inspiring">
            </span>
          things.
        </h3>
      </div>
      <div class="we-do__hero__description col-12 col-md-7">
        <p>
          Lorem ipsum dolor sit amet, consectetur adipiscing elit. Maecenas varius tortor nibh, sit amet tempor nibh finibus et.
        </p>
      </div>
      <div class="col-12 we-do__hero__img">
        <div class="ibg-js bg">
          <img src="images/about/about-img.png"  alt="img">
        </div> 
      </div>
    </div>
  </div>
</section>
    
<section class="numbers"> 
    <div class="container position-relative left-position-text__container">
      
        <div class="left-position-text">
            <span>We in numbers</span>
        </div>
      {{!-- счетчики   --}}
        <div class="numbers__wrapper row justify-content-center">
          
        {{#each numbers}}
            {{> ../partials/about-numbers}}
         {{/each}} 
       
       
        </div>
      {{!-- счетчики   --}}
          
    </div>
</section>
    
<section class="our-team margin-footer">
    <div class="container position-relative left-position-text__container ">
       <div class="row no-gutters justify-content-between">
        <div class="left-position-text">
            <span>Our team</span>
        </div>

        <div class="our-team__title"> Lorem ipsum dolor sit amet, consectetur adipisicing elit. Molestias sint, fugit omnis
            nobis at illum a odio consectetur debitis blanditiis.</div>
        
        <div class="our-team__wrapper row no-gutters"> 

       {{#each item}}
 
  <div class="our-team__item_wrapper ">  
                  
      <div class="our-team__item " id="{{id}}">   
                    
                     <a href="{{href}}">
                      <div class="our-team__img" style="background-image: url('{{img}}')"></div>
                      <h3>{{name-1}} <span class="red-span">{{name-2}}</span>{{name-3}}</h3>  
                      <p>{{dcr}}</p> 
                     </a>
      </div> 
             
              <div>
                <div class="sliding-text scrolling__text" id="{{id}}-text">
                      {{dcr}}
                </div>
              </div> 
             
  </div>
     
       {{/each}}  
      
      <div class="our-team__item_wrapper empty ">
        <div class="our-team__item " >
            <div class="our-team__img" ></div>
            <h3></h3>
            <p></p>
        </div>
      </div>

        <div class="our-way__content d-flex justify-content-between justify-content-center">  
          {{#each our-way}}
            {{> ../partials/our-way-tile}} 
          {{/each}} 
        </div>

      </div>{{!-- our-team__item_wrapper --}}
    </div>
</section>  



{{> ../common/footer }}
<!-- icons -->
{{> ../partials/icons }}
<!-- /icons -->
</body>
</html>
